<% include ./partials/header.ejs %>


    <% blogs.forEach(function(blog){ %>
        <div>
            <h2><%= blog.title %></h2>
            <img src="<%= blog.image %>">
            <div><%= blog.created.toDateString() %></div>
            <p><%= blog.body.substring(0,100) %>...</p>
            <a href="/blogs/<%= blog._id %>">Read More</a>
        </div>
    <% });%>





<% include ./partials/footer.ejs %>